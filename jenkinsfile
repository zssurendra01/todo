pipeline {
    agent {
        label "SLAVE"
    }
    stages {
        stage('download npm') {
            steps {
                sh '''
                 npm install
                '''
            }
        }
        
        stage('seeing artifacts') {
            steps {
                sh '''
                zip -r todo.zip node_modules server.js *
                '''
            }
        }
    }
}   
